<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kolhar.dabbalisttracker">

    <uses-permission
        android:name="android.permission.READ_CONTACTS"
        android:required="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.kolhar.dabbalisttracker.FirstScreenActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.kolhar.dabbalisttracker.NewDabbaActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.kolhar.dabbalisttracker.WinnerActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.kolhar.dabbalisttracker.LiveDabbaActivity"
            android:label="@string/app_name" />

    </application>

</manifest>